[
    { "keys": [";"], "type": "function", "command": "enter_command_line_mode" },
    { "keys": ["h"], "type": "function", "command": "focus_previous" },
    { "keys": ["j"], "type": "function", "command": "move_down" },
    { "keys": ["k"], "type": "function", "command": "move_up" },
    { "keys": ["l"], "type": "function", "command": "focus_next" },
    { "keys": ["K"], "type": "function", "command": "page_up" },
    { "keys": ["J"], "type": "function", "command": "page_down" },
    { "keys": ["ENTER"], "type": "function", "command": "activate" },
    { "keys": ["DELETE", "BACKSPACE"], "type": "function", "command": "delete" },
    { "keys": [" "], "command": "todo toggle $n" },
    { "keys": ["n"], "type": "function", "command": "next_link" },
    { "keys": ["p"], "type": "function", "command": "previous_link" },
    { "keys": ["o"], "type": "function", "command": "open_link" },
    { "keys": ["c"], "type": "function", "command": "toggle_console" },
    { "keys": ["m"], "type": "function", "command": "toggle_metadata" },
    { "keys": ["q"], "type": "prompt", "command": "exit" },
    { "keys": ["?"], "type": "prompt", "command": "help" },
    { "keys": ["/"], "type": "prompt", "command": "search \"\"", "cursorPosition": -2 },
    { "keys": ["N"], "type": "prompt", "command": "mknote \"\"", "cursorPosition": -2 },
    { "keys": ["T"], "type": "prompt", "command": "mktodo \"\"", "cursorPosition": -2 },
    { "keys": ["B"], "type": "prompt", "command": "mkbook \"\"", "cursorPosition": -2 },
    { "keys": ["C"], "type": "prompt", "command": "cp $n \"\"", "cursorPosition": -2 },
    { "keys": ["M"], "type": "prompt", "command": "mv $n \"\"", "cursorPosition": -2 },
    { "keys": ["R"], "type": "prompt", "command": "ren $n \"\"", "cursorPosition": -2 },
    { "keys": ["D"], "type": "prompt", "command": "rmnote $n", "cursorPosition": -2 }
]
